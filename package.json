{
  "name": "cfp-calc",
  "version": "0.1.0",
  "description": "A carbon footprint calculation library for those living in Japan",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install && npm run build",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "export:footprints": "ts-node ./data/footprint2ts.ts",
    "export:parameters": "ts-node ./data/parameter2ts.ts",
    "export:options": "ts-node ./data/option2ts.ts",
    "export:all": "npm run export:footprints && npm run export:parameters && npm run export:options",
    "test": "jest",
    "commit-msg": "commitlint -e $GIT_PARAMS",
    "doc": "typedoc"
  },
  "author": "Naoto Komiya",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@microsoft/tsdoc": "^0.14.2",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "csv-parse": "^5.3.8",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  }
}
