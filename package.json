{
  "name": "cfp-calc",
  "version": "0.2.4",
  "description": "A carbon footprint calculation library for those living in Japan",
  "author": "Naoto Komiya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naoji3x/cfp-calc"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/naoji3x/cfp-calc/issues"
  },
  "homepage": "https://naoji3x.github.io/cfp-calc",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:ts": "tsc",
    "build:doc": "shx cp README.md dist",
    "build:package": "shx cp package.json dist",
    "build": "run-p build:**",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "export:footprints": "ts-node ./data/footprint2ts.ts",
    "export:parameters": "ts-node ./data/parameter2ts.ts",
    "export:options": "ts-node ./data/option2ts.ts",
    "export": "run-p export:**",
    "test": "jest",
    "coverage": "jest --coverage",
    "commit-msg": "commitlint -e $GIT_PARAMS",
    "docs": "typedoc",
    "npm": "npm run build && cd dist && npm publish && cd .."
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@microsoft/tsdoc": "^0.14.2",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "csv-parse": "^5.3.8",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "xlsx": "^0.18.5"
  }
}
